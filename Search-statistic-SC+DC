						Search obj = new Search(inputParameters);
						obj.queryStartEntry();
						
						System.out.println("***********************\n\n");
						numOfCombinations ++;
					}
				}
			}
		}
		
		
		System.out.println("numOfCombinations: " + numOfCombinations);
	
